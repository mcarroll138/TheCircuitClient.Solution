@{
    ViewData["Title"] = "CalendarEvents";
}

<h1>All Calendar Events:</h1>

<ol>
    @foreach (CalendarEvent calendarEvent in Model)
    {
        <li><strong>@calendarEvent.Name</strong> the @calendarEvent.Description | <a
                href="/CalendarEvents/Details/@(calendarEvent.CalendarEventId)">View Details</a></li>
    }
</ol>

<h3>Actions:</h3>
<p>@Html.ActionLink("Add a new event", "Create")</p>